{
  "name": "memoriadb",
  "version": "1.0.5",
  "description": "A lightweight, in-memory database library for Node.js, designed for simplicity and ease of use.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
