  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kingrashy12/memoriadb.git"
  },
  "homepage": "https://github.com/Kingrashy12/memoriadb.git#reame",
  "bugs": {
    "url": "https://github.com/Kingrashy12/memoriadb.git/issues"
  },
  "keywords": [
    "in-memory",
    "database",
    "data"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Rapheal Chizitere",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.7",
    "rollup": "^4.31.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-visualizer": "^5.14.0"
  }
